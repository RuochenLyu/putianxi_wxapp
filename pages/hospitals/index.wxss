page {
  background: #F2F2F3;
}

.header {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  padding: 20rpx 50rpx 40rpx;
  background-color: #fff;
  z-index: 999;
  border-bottom: 1px solid #eee;
}

.searchbar {
  display: flex;
  height: 100rpx;
  background-color: #F2F2F3;
  border-radius: 25rpx;
}

.searchbar-icon {
  width: 100rpx;
  height: 100rpx;
  background: center / 50rpx no-repeat url('data:image/svg+xml;utf8,<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#aaa" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-search"><circle cx="11" cy="11" r="8"></circle><line x1="21" y1="21" x2="16.65" y2="16.65"></line></svg>');
}

.searchbar-cancel {
  width: 100rpx;
  height: 100rpx;
  background: center / 50rpx no-repeat url('data:image/svg+xml;utf8,<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>');
}

.searchbar-input {
  flex: 1;
  height: 100rpx;
  padding: 0;
  font-size: 36rpx;
}

.main {
  margin: 200rpx 50rpx 0;
}

.main-desc {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  color: #777;
  font-size: 24rpx;
  line-height: 48rpx;
}

