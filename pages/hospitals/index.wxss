.main-desc-border {
  width: 100%;
  margin-bottom: -34rpx;
  border-bottom: 1px solid #ccc;
}

.hospital {
  margin: 30rpx 0;
  padding: 30rpx;
  color: #000;
  border-radius: 30rpx;
  box-shadow: 4rpx 4rpx 50rpx -12rpx #ccc;
  background: #fff;
  transition: 200ms all;
}

.hospital.active {
  transform: scale(0.98);
  color: #fff;
  background-color: #E25D56;
}

.hospital.active .hospital-info {
  color: #fff;
}

.hospital-name {
  font-weight: bold;
}

.hospital-info {
  color: #777;
  font-size: 28rpx;
  transition: 200ms all;
}

